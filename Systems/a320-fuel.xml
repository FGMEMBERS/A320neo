<!-- ###########################################################  -->
<!-- # Fuel system for A320neo                                 #  -->
<!-- # 2012-02-16 - By Jon A. Ortuondo (Bicyus) for A320neo    #  -->
<!-- ###########################################################  -->

<system name="fuel">
	<channel name="fuel">

<!-- Engines Feeding Pipes -->
		<!-- Add fuel to Left Pipes (tank[7])-->
		<summer name="propulsion/tank[7]/external-flow-rate">
			<!-- From Pipes to Jettison -->
			<!--input>-propulsion/tank[7]/external-flow-rate/jettison</input-->
			<!-- From Pipes Back to Left Inner Wing Tank -->
			<input>-propulsion/tank[7]/external-flow-rate/tank[2]</input>

			<!-- X-Feed -->
			<input>-propulsion/tank[8]/external-flow-rate/tank[7]</input>
			<input>propulsion/tank[7]/external-flow-rate/tank[8]</input>

			<!-- From Left Inner Wing tank -->
			<input>propulsion/tank[2]/external-flow-rate/tank[7]</input>
			<!-- From Center tank -->
			<input>propulsion/tank[3]/external-flow-rate/tank[7]</input>

			<output>propulsion/tank[7]/external-flow-rate-pps</output>
		</summer>

		<!-- Add fuel to Right Pipes (tank[8])-->
		<summer name="propulsion/tank[8]/external-flow-rate">
			<!-- From Pipes to Jettison -->
			<!--input>-propulsion/tank[8]/external-flow-rate/jettison</input-->
			<!-- From Pipes Back to Right Inner Wing Tank -->
			<input>-propulsion/tank[8]/external-flow-rate/tank[4]</input>

			<!-- X-Feed -->
			<input>-propulsion/tank[8]/external-flow-rate/tank[7]</input>
			<input>propulsion/tank[7]/external-flow-rate/tank[8]</input>

			<!-- From Right Inner Wing tank -->
			<input>propulsion/tank[4]/external-flow-rate/tank[8]</input>
			<!-- From Center tank -->
			<input>propulsion/tank[3]/external-flow-rate/tank[8]</input>

			<output>propulsion/tank[8]/external-flow-rate-pps</output>
		</summer>

<!-- Inner tanks -->

		<!-- LEFT INNER Wing (tank[2])-->
		<summer name="propulsion/tank[2]/external-flow-rate">

			<!-- To Left Pipes -->
			<input>-propulsion/tank[2]/external-flow-rate/tank[7]</input>
			<!-- From Left Pipes -->
			<input>propulsion/tank[7]/external-flow-rate/tank[2]</input>
			<!-- From OUTER Wing -->
			<input>propulsion/tank[1]/external-flow-rate/tank[2]</input>

			<output>propulsion/tank[2]/external-flow-rate-pps</output>
		</summer>

		<!-- Right INNER Wing (tank[4])-->
		<summer name="propulsion/tank[4]/external-flow-rate">

			<!-- To Left Pipes -->
			<input>-propulsion/tank[4]/external-flow-rate/tank[8]</input>
			<!-- From Left Pipes -->
			<input>propulsion/tank[8]/external-flow-rate/tank[4]</input>
			<!-- From OUTER Wing -->
			<input>propulsion/tank[6]/external-flow-rate/tank[4]</input>

			<output>propulsion/tank[4]/external-flow-rate-pps</output>
		</summer>

<!-- Outer Tanks -->
		<!-- LEFT OUTER Wing (tank[1])-->
		<summer name="propulsion/tank[1]/external-flow-rate">
			<!-- To Left INNER Wing -->
			<input>-propulsion/tank[1]/external-flow-rate/tank[2]</input>
			<output>propulsion/tank[1]/external-flow-rate-pps</output>
		</summer>

		<!-- LEFT OUTER Wing (tank[5])-->
		<summer name="propulsion/tank[5]/external-flow-rate">
			<!-- To Left INNER Wing -->
			<input>-propulsion/tank[5]/external-flow-rate/tank[4]</input>
			<output>propulsion/tank[5]/external-flow-rate-pps</output>
		</summer>

<!-- Center Tank -->
		<!-- CENTER (tank[5])-->
		<summer name="propulsion/tank[3]/external-flow-rate">
			<!-- To Left INNER Wing -->
			<input>-propulsion/tank[3]/external-flow-rate/tank[7]</input>
			<input>-propulsion/tank[3]/external-flow-rate/tank[8]</input>
			<output>propulsion/tank[3]/external-flow-rate-pps</output>
		</summer>


	   
	</channel>
	
</system>
