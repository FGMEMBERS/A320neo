<?xml version="1.0"?>


<PropertyList>
    <name>PLAN</name>
    <w-base>256</w-base>
    <h-base>256</h-base>
    <params>
      <inputs>
        <lh-vor-adf>instrumentation/efis[0]/inputs/vor-adf-1</lh-vor-adf>
        <rh-vor-adf>instrumentation/efis[0]/inputs/vor-adf-2</rh-vor-adf>
      </inputs>
      <nd-range>instrumentation/nd[0]/range</nd-range>
    </params>

    <layers>

<!-- ROSE mode -->

        <layer>
            <name>hdg1</name>
            <w>220</w>
            <h>220</h>
            <texture>
                <path>Aircraft/A320neo/Models/Instruments/MFD/hsi.png</path>
                <x1>0</x1>
                <y1>0</y1>
                <x2>1</x2>
                <y2>1</y2>
            </texture>
            <transformations>
                <transformation>
                    <type>y-shift</type>
                    <offset>-12</offset>
                </transformation>
                <!-- transformation>
                    <type>rotation</type>
                    <property>orientation/heading-magnetic-deg</property>
                    <scale>-1</scale>
                </transformation -->
            </transformations>
            <emissive>true</emissive>
        </layer>

        <layer>
            <condition>
               <and>
                <equals>
		    <property>flight-management/control/lat-ctrl</property>
                    <value>man-set</value>
                </equals>
                </and>
            </condition>
            <name>hdgbug</name>
            <emissive>true</emissive>
            <w>21</w>
            <h>21</h>
            <texture>
                <path>Aircraft/A320neo/Models/Instruments/MFD/airbus_symbols.png</path>
                <x1>0.035</x1>
                <y1>0.810</y1>
                <x2>0.145</x2>
                <y2>0.970</y2>
            </texture>
           <transformations>
            <transformation>
                <type>y-shift</type>
                <offset>-12</offset>
            </transformation>
            <transformation>
                <type>rotation</type>
		<property>flight-management/fcu-values/hdg</property>
            </transformation>
	    <!-- transformation>
                <type>rotation</type>
                <property>orientation/heading-magnetic-deg</property>
                <scale>-1</scale>
            </transformation -->
            <transformation><!--shift rotation origin-->
                <type>y-shift</type>
                <!-- offset>48</offset -->
                <offset>82</offset>
            </transformation>
        </transformations>
        </layer>

        <layer>
            <condition>
              <and>
                <equals>
                  <property alias="/params/inputs/lh-vor-adf"/>
                  <value>1</value>
                </equals>
                <equals>
                    <property>instrumentation/afs/approach-radio-lh</property>
                    <value>VOR</value>
                </equals>
                <equals>
                  <property>instrumentation/nav[0]/in-range</property>
                  <value>1</value>
                </equals>
              </and>
            </condition>
            <name>Lvor head</name>
            <emissive>true</emissive>
            <w>10</w>
            <h>30</h>
            <texture>
                <path>Aircraft/A320neo/Models/Instruments/MFD/airbus_symbols.png</path>
                <x1>0.065</x1>
                <y1>0.380</y1>
                <x2>0.180</x2>
                <y2>0.570</y2>
            </texture>
           <transformations>
            <transformation>
                <type>y-shift</type>
                <offset>-12</offset>
            </transformation>
            <!-- transformation>
                <type>rotation</type>
                <property>orientation/heading-deg</property>
                <scale>-1</scale>
            </transformation -->
            <transformation>
                <type>rotation</type>
                <property>instrumentation/nav/heading-deg</property>
            </transformation>
            <transformation><!--shift rotation origin-->
                <type>y-shift</type>
                <offset>70</offset>
            </transformation>
        </transformations>
        </layer>

        <layer>
            <condition>
              <and>
                <equals>
                  <property alias="/params/inputs/lh-vor-adf"/>
                  <value>1</value>
                </equals>
                <equals>
                    <property>instrumentation/afs/approach-radio-lh</property>
                    <value>VOR</value>
                </equals>
                <equals>
                  <property>instrumentation/nav[0]/in-range</property>
                  <value>1</value>
                </equals>
              </and>
            </condition>
            <name>Lvor tail</name>
            <emissive>true</emissive>
            <w>10</w>
            <h>30</h>
            <texture>
                <path>Aircraft/A320neo/Models/Instruments/MFD/airbus_symbols.png</path>
                <x1>0.065</x1>
                <y1>0.380</y1>
                <x2>0.180</x2>
                <y2>0.570</y2>
            </texture>
           <transformations>
            <transformation>
                <type>y-shift</type>
                <offset>-12</offset>
            </transformation>
            <!-- transformation>
                <type>rotation</type>
                <property>orientation/heading-deg</property>
                <scale>-1</scale>
            </transformation -->
            <transformation>
                <type>rotation</type>
                <property>instrumentation/nav/heading-deg</property>
            </transformation>
            <transformation><!--shift rotation origin-->
                <type>y-shift</type>
                <offset>-70</offset>
            </transformation>
        </transformations>
        </layer>

        <layer>
            <condition>
                <and>
                  <equals>
                        <property alias="/params/inputs/lh-vor-adf"/>
                        <value>1</value>
                  </equals>
                  <equals>
                    <property>instrumentation/afs/approach-radio-lh</property>
                    <value>ADF</value>
                  </equals>
                  <equals>
                    <property>instrumentation/adf[0]/in-range</property>
                    <value>1</value>
                  </equals>
                </and>
            </condition>
            <name>Ladf head</name>
            <emissive>true</emissive>
            <color>
                <red>0.0</red>
                <green>1.0</green>
                <blue>0.0</blue>
            </color>
            <w>10</w>
            <h>24</h>
            <texture>
                <path>Aircraft/A320neo/Models/Instruments/MFD/symbols.png</path>
                <x1>0.301</x1>
                <y1>0.270</y1>
                <x2>0.409</x2>
                <y2>0.459</y2>
            </texture>
           <transformations>
            <transformation>
                <type>y-shift</type>
                <offset>-12</offset>
            </transformation>
            <transformation>
                <type>rotation</type>
                <property>instrumentation/adf/indicated-bearing-deg</property>
            </transformation>
            <transformation><!--shift rotation origin-->
                <type>y-shift</type>
                <offset>80</offset>
            </transformation>
        </transformations>
        </layer>

        <layer>
            <condition>
                <and>
                  <equals>
                        <property alias="/params/inputs/lh-vor-adf"/>
                        <value>1</value>
                  </equals>
                <equals>
                    <property>instrumentation/afs/approach-radio-lh</property>
                    <value>ADF</value>
                  </equals>
                  <equals>
                    <property>instrumentation/adf[0]/in-range</property>
                    <value>1</value>
                  </equals>
                </and>
            </condition>
            <name>Ladf tail</name>
            <emissive>true</emissive>
            <w>10</w>
            <h>24</h>
            <texture>
                <path>Aircraft/A320neo/Models/Instruments/MFD/symbols.png</path>
                <x1>0.301</x1>
                <y1>0.061</y1>
                <x2>0.409</x2>
                <y2>0.250</y2>
            </texture>
           <transformations>
            <transformation>
                <type>y-shift</type>
                <offset>-12</offset>
            </transformation>
            <transformation>
                <type>rotation</type>
                <property>instrumentation/adf/indicated-bearing-deg</property>
            </transformation>
            <transformation><!--shift rotation origin-->
                <type>y-shift</type>
                <offset>-80</offset>
            </transformation>
        </transformations>
        </layer>

        <layer>
            <condition>
                <and>
                  <equals>
                        <property alias="/params/inputs/rh-vor-adf"/>
                        <value>1</value>
                  </equals>
                <equals>
                    <property>instrumentation/afs/approach-radio-rh</property>
                    <value>VOR</value>
                  </equals>
                  <equals>
                    <property>instrumentation/nav[1]/in-range</property>
                    <value>1</value>
                  </equals>
                </and>
            </condition>
            <name>Rvor head</name>
            <emissive>true</emissive>
            <w>10</w>
            <h>30</h>
            <texture>
                <path>Aircraft/A320neo/Models/Instruments/MFD/airbus_symbols.png</path>
                <x1>0.310</x1>
                <y1>0.380</y1>
                <x2>0.410</x2>
                <y2>0.575</y2>
            </texture>
           <transformations>
            <transformation>
                <type>y-shift</type>
                <offset>-12</offset>
            </transformation>
            <!-- transformation>
                <type>rotation</type>
                <property>orientation/heading-deg</property>
                <scale>-1</scale>
            </transformation -->
            <transformation>
                <type>rotation</type>
                <property>instrumentation/nav[1]/heading-deg</property>
            </transformation>
            <transformation><!--shift rotation origin-->
                <type>y-shift</type>
                <offset>70</offset>
            </transformation>
        </transformations>
        </layer>

        <layer>
            <condition>
                <and>
                  <equals>
                        <property alias="/params/inputs/rh-vor-adf"/>
                        <value>1</value>
                  </equals>
                <equals>
                    <property>instrumentation/afs/approach-radio-rh</property>
                    <value>VOR</value>
                  </equals>
                  <equals>
                    <property>instrumentation/nav[1]/in-range</property>
                    <value>1</value>
                  </equals>
                </and>
            </condition>
            <name>Rvor tail</name>
            <emissive>true</emissive>
            <w>10</w>
            <h>30</h>
            <texture>
                <path>Aircraft/A320neo/Models/Instruments/MFD/airbus_symbols.png</path>
                <x1>0.310</x1>
                <y1>0.380</y1>
                <x2>0.410</x2>
                <y2>0.575</y2>
            </texture>
           <transformations>
            <transformation>
                <type>y-shift</type>
                <offset>-12</offset>
            </transformation>
            <!-- transformation>
                <type>rotation</type>
                <property>orientation/heading-deg</property>
                <scale>-1</scale>
            </transformation -->
            <transformation>
                <type>rotation</type>
                <property>instrumentation/nav[1]/heading-deg</property>
            </transformation>
            <transformation><!--shift rotation origin-->
                <type>y-shift</type>
                <offset>-70</offset>
            </transformation>
        </transformations>
        </layer>

        <layer>
            <condition>
                <and>
                  <equals>
                        <property alias="/params/inputs/rh-vor-adf"/>
                        <value>1</value>
                  </equals>
                <equals>
                    <property>instrumentation/afs/approach-radio-rh</property>
                    <value>ADF</value>
                  </equals>
                  <equals>
                    <property>instrumentation/adf[1]/in-range</property>
                    <value>1</value>
                  </equals>
                </and>
            </condition>
            <name>Radf head</name>
            <emissive>true</emissive>
            <w>10</w>
            <h>24</h>
            <texture>
                <path>Aircraft/A320neo/Models/Instruments/MFD/symbols.png</path>
                <x1>0.218</x1>
                <y1>0.297</y1>
                <x2>0.287</x2>
                <y2>0.454</y2>
            </texture>
           <transformations>
            <transformation>
                <type>y-shift</type>
                <offset>-12</offset>
            </transformation>
            <transformation>
                <type>rotation</type>
                <property>instrumentation/adf/indicated-bearing-deg</property>
            </transformation>
            <transformation><!--shift rotation origin-->
                <type>y-shift</type>
                <offset>80</offset>
            </transformation>
        </transformations>
        </layer>

        <layer>
            <condition>
                <and>
                  <equals>
                        <property alias="/params/inputs/rh-vor-adf"/>
                        <value>1</value>
                  </equals>
                <equals>
                    <property>instrumentation/afs/approach-radio-rh</property>
                    <value>ADF</value>
                  </equals>
                  <equals>
                    <property>instrumentation/adf[1]/in-range</property>
                    <value>1</value>
                  </equals>
                </and>
            </condition>
            <name>Radf tail</name>
            <emissive>true</emissive>
            <w>10</w>
            <h>24</h>
            <texture>
                <path>Aircraft/A320neo/Models/Instruments/MFD/symbols.png</path>
                <x1>0.218</x1>
                <y1>0.093</y1>
                <x2>0.287</x2>
                <y2>0.250</y2>
            </texture>
           <transformations>
            <transformation>
                <type>y-shift</type>
                <offset>-12</offset>
            </transformation>
            <transformation>
                <type>rotation</type>
                <property>instrumentation/adf/indicated-bearing-deg</property>
            </transformation>
            <transformation><!--shift rotation origin-->
                <type>y-shift</type>
                <offset>-80</offset>
            </transformation>
        </transformations>
        </layer>

        
        
        
        <!-- the three pseudo WPTs -->
        <layer>
          <condition>
              <equals>
                <property>instrumentation/radar/toc/valid</property>
                <value>1</value>
              </equals>
          </condition>
          <name>toc</name>
          <w>15</w>
          <h>15</h>
          <texture>
            <path>Aircraft/A320neo/Models/Instruments/MFD/airbus_symbols.png</path>
            <x1>0.060</x1>
            <y1>0.670</y1>
            <x2>0.150</x2>
            <y2>0.760</y2>
          </texture>
          <emissive>true</emissive>
          <transformations>
            <transformation>
               <type>y-shift</type>
	       <offset>-22</offset> <!-- was -12 SAH 2012-03-08 -->
            </transformation>
            <transformation>
                <type>rotation</type>
                <property>instrumentation/radar/toc/brg-offset</property>
            </transformation>
            <transformation>
               <type>y-shift</type>
               <property>instrumentation/radar/toc/dist-norm</property>
               <scale>80</scale>
            </transformation>
            <transformation>
                <type>rotation</type>
                <property>instrumentation/radar/toc/brg-offset</property>
                <scale>-1.0</scale>
            </transformation>
          </transformations>
        </layer>
        
        <layer>
          <condition>
              <equals>
                <property>instrumentation/radar/tod/valid</property>
                <value>1</value>
              </equals>
          </condition>
          <name>tod</name>
          <w>15</w>
          <h>15</h>
          <texture>
            <path>Aircraft/A320neo/Models/Instruments/MFD/airbus_symbols.png</path>
            <x1>0.185</x1>
            <y1>0.665</y1>
            <x2>0.275</x2>
            <y2>0.755</y2>
          </texture>
          <emissive>true</emissive>
          <transformations>
            <transformation>
               <type>y-shift</type>
               <offset>-22</offset>  <!-- was -12 SAH 2012-03-08 -->
            </transformation>
            <transformation>
                <type>rotation</type>
                <property>instrumentation/radar/tod/brg-offset</property>
            </transformation>
            <transformation>
               <type>y-shift</type>
               <property>instrumentation/radar/tod/dist-norm</property>
               <scale>80</scale>
            </transformation>
            <transformation>
                <type>rotation</type>
                <property>instrumentation/radar/tod/brg-offset</property>
                <scale>-1.0</scale>
            </transformation>
          </transformations>
        </layer>
        
        <layer>
          <condition>
              <equals>
                <property>instrumentation/radar/decel/valid</property>
                <value>1</value>
              </equals>
          </condition>
          <name>decel</name>
          <w>12</w>
          <h>12</h>
          <texture>
            <path>Aircraft/A320neo/Models/Instruments/MFD/airbus_symbols-bgnd.png</path>
            <x1>0.300</x1>
            <y1>0.600</y1>
            <x2>0.410</x2>
            <y2>0.800</y2>
          </texture>
          <emissive>true</emissive>
          <transformations>
            <transformation>
               <type>y-shift</type>
               <offset>-22</offset>  <!-- was -12 SAH 2012-03-08 -->
            </transformation>
            <transformation>
                <type>rotation</type>
                <property>instrumentation/radar/decel/brg-offset</property>
            </transformation>
            <transformation>
               <type>y-shift</type>
               <property>instrumentation/radar/decel/dist-norm</property>
               <scale>80</scale>
            </transformation>
            <transformation>
                <type>rotation</type>
                <property>instrumentation/radar/decel/brg-offset</property>
                <scale>-1.0</scale>
            </transformation>
          </transformations>
        </layer>
        
        <layer>
            <name>aircraft symbol</name>
            <w>30</w>
            <h>30</h>
            <texture>
                <path>Aircraft/A320neo/Models/Instruments/MFD/airbus_symbols.png</path>
                <x1>0.332</x1>
                <y1>0.111</y1>
                <x2>0.494</x2>
		<y2>0.326</y2>
            </texture>
            <transformations>
                <transformation>
                    <type>y-shift</type>
                    <offset>-17</offset>
                </transformation>
            </transformations>
            <emissive>true</emissive>
        </layer>

    </layers>

</PropertyList>
