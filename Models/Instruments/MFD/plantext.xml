<?xml version="1.0"?>


<PropertyList>
    <name>PLANtext</name>
    <w-base>256</w-base>
    <h-base>256</h-base>
    <params>
      <inputs>
        <lh-vor-adf>instrumentation/efis[0]/inputs/vor-adf-1</lh-vor-adf>
        <rh-vor-adf>instrumentation/efis[0]/inputs/vor-adf-2</rh-vor-adf>
      </inputs>
      <nd-range>instrumentation/nd[0]/range</nd-range>
    </params>
    
    <layers>

        <layer>
            <name>bkgrd</name>
            <w>256</w>
            <h>256</h>
            <texture>
                <path>Aircraft/A320neo/Models/Instruments/MFD/transparent.png</path>
                <x1>0.0</x1>
                <y1>0.0</y1>
                <x2>1</x2>
                <y2>1</y2>
            </texture>
        <emissive>true</emissive>
        </layer>

        <layer>
            <name>GSPD</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>8.5</point-size>
            <color>
                <red>1.0</red>
                <green>1.0</green>
                <blue>1.0</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
                    <property>velocities/groundspeed-kt</property>
                    <format>GS%3.0f</format>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-120</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>115</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>TAS</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>8.5</point-size>
            <color>
                <red>1.0</red>
                <green>1.0</green>
                <blue>1.0</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
                    <property>velocities/uBody-fps</property>
                    <scale>0.59248380</scale>
                    <format>TAS%3.0f</format>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-80</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>115</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <condition>
                <greater-than>
                    <property>environment/wind-speed-kt</property>
                    <value>0.99</value>
                </greater-than>
            </condition>
            <name>Wind</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>8</point-size>
            <color>
                <red>0.1</red>
                <green>1.0</green>
                <blue>0.1</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
                    <property>environment/wind-from-heading-deg</property>
                    <format>%3.0f</format>
                </chunk>
                <chunk>
                    <type>literal</type>
                    <text>*/</text>
                </chunk>
                <chunk>
                    <type>number-value</type>
                    <property>environment/wind-speed-kt</property>
                    <format>%3.0f</format>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-120</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>100</offset>
                </transformation>
            </transformations>
        </layer>


<!-- ROSE -->
        <layer>
            <name>WXR range</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>8</point-size>
            <color>
                <red>0.8</red>
                <green>0.8</green>
                <blue>0.8</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
		    <property alias="/params/nd-range"/>
                    <format>%3.0f</format>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-30</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>15</offset>
                </transformation>
            </transformations>
        </layer>

