			</and>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>mach.ind</object-name>
		<condition>
			<and>
				<equals>
					<property>/flight-management/control/spd-ctrl</property>
					<value>man-set</value>
				</equals>
				<equals>
					<property>/flight-management/control/spd-mode</property>
					<value>mach</value>
				</equals>
			</and>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>spd.ind</object-name>
		<condition>
			<and>
				<equals>
					<property>/flight-management/control/spd-ctrl</property>
