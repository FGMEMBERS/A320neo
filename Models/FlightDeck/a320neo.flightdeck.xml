	<!-- Knobs' Pick Animations -->
	
	<!-- IMPORTANT - USE SCROLL WHEEL ON MOUSE OF TOUCH PAD EDGE TO SCROLL, AND CLICK TO SELECT -->
	
	<animation>
		<type>pick</type>
		<object-name>spd-sel</object-name>
		<action>
			<button>3</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					var spd_mode = getprop("/flight-management/control/spd-mode");
					
					if (spd_mode == "ias") {
					
						var ias = getprop("/flight-management/fcu-values/ias");
						
						if (350 > ias)
							setprop("/flight-management/fcu-values/ias", ias + 1);
					
					} else {
					
						var mach = getprop("/flight-management/fcu-values/mach");
						
						if (0.82 > mach)
							setprop("/flight-management/fcu-values/mach", mach + 0.01);
					
					}
				</script>
			</binding>
		</action>
		<action>
			<button>4</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					var spd_mode = getprop("/flight-management/control/spd-mode");
					
					if (spd_mode == "ias") {
					
						var ias = getprop("/flight-management/fcu-values/ias");
						
						if (ias > 0)
							setprop("/flight-management/fcu-values/ias", ias - 1);
					
					} else {
					
						var mach = getprop("/flight-management/fcu-values/mach");
						
						if (mach > 0)
							setprop("/flight-management/fcu-values/mach", mach - 0.01);
					
					}
				</script>
			</binding>
		</action>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-cycle</command>
				<property>/flight-management/control/spd-ctrl</property>
				<value>man-set</value>
				<value>fmgc</value>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>hdg-sel</object-name>
		<action>
			<button>3</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>nasal</command>
				<script>
				
					var hdg_prop = "/flight-management/fcu-values/hdg";
				
					if (359 > getprop(hdg_prop))
						setprop(hdg_prop, getprop(hdg_prop) + 1);
					else
						setprop(hdg_prop, 0);
					
				</script>
			</binding>
		</action>
		<action>
			<button>4</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					
					var hdg_prop = "/flight-management/fcu-values/hdg";
				
					if (getprop(hdg_prop) > 0)
						setprop(hdg_prop, getprop(hdg_prop) - 1);
					else
						setprop(hdg_prop, 359);
					
				</script>
			</binding>
		</action>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-cycle</command>
				<property>/flight-management/control/lat-ctrl</property>
				<value>man-set</value>
				<value>fmgc</value>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>alt-sel</object-name>
		<action>
			<button>3</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>nasal</command>
				<script>
				
					var step = getprop("/flight-management/control/alt-sel-mode");
				
					var alt_prop = "/flight-management/fcu-values/alt";
				
					if (41000 > getprop(alt_prop))
						setprop(alt_prop, getprop(alt_prop) + step);
					
				</script>
			</binding>
		</action>
		<action>
			<button>4</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					
					var step = getprop("/flight-management/control/alt-sel-mode");
					
					var alt_prop = "/flight-management/fcu-values/alt";
				
					if (getprop(alt_prop) > 0)
						setprop(alt_prop, getprop(alt_prop) - step);
					
				</script>
			</binding>
		</action>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-cycle</command>
				<property>/flight-management/control/ver-ctrl</property>
				<value>man-set</value>
				<value>fmgc</value>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>vs-sel</object-name>
		<action>
			<button>3</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>nasal</command>
				<script>
				
					var sub_mode = getprop("/flight-management/control/ver-sub");
				
					var vs_prop = "/flight-management/fcu-values/vs";
					
					var fpa_prop = "/flight-management/fcu-values/fpa";
				
					if (sub_mode == "vs") {
					
						if (3000 > getprop(vs_prop))
							setprop(vs_prop, getprop(vs_prop) + 100);
					
					} else {
					
						if (15 > getprop(fpa_prop))
							setprop(fpa_prop, getprop(fpa_prop) + 1);
					
					}
					
				</script>
			</binding>
		</action>
		<action>
			<button>4</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					
					var sub_mode = getprop("/flight-management/control/ver-sub");
				
					var vs_prop = "/flight-management/fcu-values/vs";
					
					var fpa_prop = "/flight-management/fcu-values/fpa";
				
					if (sub_mode == "vs") {
					
						if (getprop(vs_prop) > -2500)
							setprop(vs_prop, getprop(vs_prop) - 100);
					
					} else {
					
						if (getprop(fpa_prop) > -10)
							setprop(fpa_prop, getprop(fpa_prop) - 1);
					
					}
					
				</script>
			</binding>
		</action>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
				
					setprop("/flight-management/fcu-values/alt", getprop("/position/altitude-ft"));
					
					setprop("/flight-management/fcu-values/vs", 0);
					
					setprop("/flight-management/fcu-values/fpa", 0);
				
					setprop("/flight-management/control/ver-ctrl", "man-set");
					
				</script>
			</binding>
		</action>
	</animation>
	
	<!-- Button Pick Animations -->
	
	<animation>
		<type>pick</type>
		<object-name>loc</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
				
					if (getprop("/flight-management/control/lat-mode") != "nav1") {
				
						setprop("/flight-management/control/lat-mode", "nav1");
	
						setprop("/flight-management/control/lat-ctrl", "man-set");
						
					} else
					
						setprop("/flight-management/control/lat-mode", "hdg");
					
				</script>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>gs</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
				
				if (getprop("/flight-management/control/ver-mode") != "ils") {
				
					setprop("/flight-management/control/ver-mode", "ils");
	
					setprop("/flight-management/control/ver-ctrl", "man-set");
				
				} else
				
					setprop("/flight-management/control/ver-mode", "alt");
					
				</script>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>exped</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
				
					var vs_fps = getprop("/velocities/vertical-speed-fps");
				
					if (vs_fps >= 0) {
				
						setprop("/flight-management/fcu-values/vs", 2800);
					
						setprop("/flight-management/fcu-values/fpa", 10);
					
					} else {
					
						etprop("/flight-management/fcu-values/vs", -2500);
					
						setprop("/flight-management/fcu-values/fpa", -10);
					
					}
					
				</script>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>spd-mach</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-cycle</command>
				<property>/flight-management/control/spd-mode</property>
				<value>mach</value>
				<value>ias</value>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>vs-fpa</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-cycle</command>
				<property>/flight-management/control/ver-sub</property>
				<value>fpa</value>
				<value>vs</value>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>alt-sel-mode</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-cycle</command>
				<property>/flight-management/control/alt-sel-mode</property>
				<value>1000</value>
				<value>100</value>
			</binding>
