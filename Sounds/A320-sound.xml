<!-- #########################   ENGINE: Revers ################################ -->
		<engine>
			<name>left reverse thrust</name>
			<path>Aircraft/A320neo/Sounds/res/reverser2.wav</path>
			<mode>looped</mode>
			<condition>
				<and>
					<greater-than>
						<property>engines/engine[0]/n1</property>
						<value>2</value>
					</greater-than>
					<greater-than>
