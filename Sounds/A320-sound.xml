    <autopilotoff>
        <name>Autopilot off</name>
        <path>Aircraft/A320neo/Sounds/res/airbus-warning.wav</path>
        <mode>once</mode>
		<type>avionics</type>
		<condition>
			<equals>
				<property>sim/alarms/AP-Disengage</property>
				<value>true</value>
			</equals>
		</condition>

        <volume>
			<property>sim/sound/effects/volume</property>
			<factor>4.0</factor>
        </volume>
     
        <reference-dist>10</reference-dist>
        <max-dist>100</max-dist>
    </autopilotoff>




		<retard2>
			<name>retard2</name>
			<path>Aircraft/A320neo/Sounds/res/retard.wav</path>
			<mode>looped</mode>
			<condition>
				<and>
					<less-than>
						<property>/position/altitude-agl-ft</property>
						<value>20</value>
					</less-than>

					<greater-than>
						<property>controls/engines/engine[0]/throttle</property>
						<value>0.0</value>
					</greater-than>
					<greater-than>
						<property>controls/engines/engine[1]/throttle</property>
						<value>0.0</value>
					</greater-than>
					<less-than>
						<property>controls/engines/engine[0]/throttle</property>
						<value>0.6</value>
					</less-than>
					<less-than>
						<property>controls/engines/engine[1]/throttle</property>
						<value>0.6</value>
					</less-than>
					<not>
						<property>/gear/gear[0]/wow</property>
						<property>/gear/gear[1]/wow</property>
						<property>/gear/gear[2]/wow</property>
					</not>
					<less-than>
						<property>velocities/airspeed-kt</property>
						<value>150</value>
					</less-than>
				</and>
			</condition>
			<volume>
				<property>/sim/sound/effects/volume</property>
			</volume>
		</retard2>

		<v1>
			<name>V1</name>
			<path>Aircraft/A320neo/Sounds/res/v1.wav</path>
			<condition>
				<and>
					<!-- At v1 airspeed -->
					<greater-than-equals>
						<property>velocities/airspeed-kt</property>
						<property>/instrumentation/fmc/vspeeds/V1</property>
					</greater-than-equals>
					<!-- While on ground -->
					<property>/gear/gear[0]/wow</property>
					<property>/gear/gear[1]/wow</property>
					<property>/gear/gear[2]/wow</property>
					<!-- With throttles in [flex]Take Off  -->
					<greater-than>
						<property>controls/engines/engine[0]/throttle</property>
						<value>0.7</value>
					</greater-than>
					<greater-than>
						<property>controls/engines/engine[1]/throttle</property>
						<value>0.7</value>
					</greater-than>
				</and>
			</condition>
			<volume>
				<property>/sim/sound/effects/volume</property>
				<factor>1.5</factor>
			</volume>
		</v1>
		<roll>
			<name>Roll</name>
			<path>Aircraft/A320neo/Sounds/res/roll.wav</path>
			<mode>looped</mode>
			<condition>
				<or>
					<!-- While on ground -->
					<property>/gear/gear[0]/wow</property>
					<property>/gear/gear[1]/wow</property>
					<property>/gear/gear[2]/wow</property>
					
				</or>
			</condition>
			<!--volume>
				<property>/sim/sound/effects/volume</property>
				<factor>1.5</factor>
			</volume-->
			<volume>
				<property>/velocities/groundspeed-kt</property>
				<type>log</type>
				<factor>0.38</factor>
				<!--max>0.9</max-->

			</volume>
			<pitch>
				<property>/velocities/groundspeed-kt</property>
				<!--offset>0</offset-->
				<type>log</type>
				<factor>0.28</factor>
				<min>0.1</min>
				<max>1.5</max>
			</pitch>
		</roll>

	</fx>
</PropertyList>
